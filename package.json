  "scripts": {
    "exercises": "npx nx build exercises && node ./dist/libs/exercises/src/index.js",
    "ts-node-typeorm": "ts-node -r tsconfig-paths/register -P ./apps/api/tsconfig.json",
    "typeorm": "npm run ts-node-typeorm -- ./node_modules/typeorm/cli -d ./apps/api/src/database/data-source.ts",
    "typeorm:migration-generate": "npm run typeorm -- migration:generate ./apps/api/src/migrations/%npm_config_name%",
    "typeorm:migration-run": "npm run typeorm -- migration:run",
    "typeorm:migration-revert": "npm run typeorm -- migration:revert",
    "seeds": "npm run ts-node-typeorm -- ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./apps/api/src/database/data-source.ts"
  },
